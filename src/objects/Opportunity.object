    <validationRules>
        <fullName>Account_name_validation</fullName>
        <active>false</active>
        <errorConditionFormula>NOT(CONTAINS( Name,  Account.Name))</errorConditionFormula>
        <errorMessage>Opportunity Name should contain Account Name</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Next_Step_update_on_Stage_change</fullName>
        <active>false</active>
        <errorConditionFormula>AND( ISCHANGED(StageName), NOT(ISCHANGED(NextStep)))</errorConditionFormula>
        <errorMessage>Next Step should be updated for Stage change.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>No_changes_after_Closed</fullName>
        <active>false</active>
        <errorConditionFormula>PRIORVALUE(IsClosed) = true</errorConditionFormula>
        <errorMessage>No changes allowed on Closed Opportunity.</errorMessage>
    </validationRules>
